{
  "name": "leafygreen-ui",
  "version": "0.0.0",
  "description": "leafyGreen UI Kit",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">= 16.20.0",
    "npm": ">= 8.19.4"
  },
  "scripts": {
    "lg": "lg",
    "init": "yarn && yarn build",
    "create-package": "lg-create",
    "prebuild": "turbo run build tsc --filter=@lg-tools/cli",
    "build": "turbo run build tsc",
    "build-storybook": "storybook build",
    "chromatic": "npx chromatic",
    "clean": "npm-run-all --parallel clean:*",
    "clean:builds": "rm -rf {packages,tools}/*/{dist,tsconfig.tsbuildinfo,stories.js}",
    "clean:modules": "rm -rf node_modules {packages,tools}/*/node_modules",
    "fix": "lg lint --fix",
    "link": "lg link",
    "lint": "lg lint",
    "prepublishOnly": "yarn build",
    "publish": "yarn changeset publish --public",
    "slackbot": "lg slackbot release",
    "start": "storybook dev -p 9001 --no-version-updates",
    "test": "lg test",
    "unlink": "lg unlink",
    "validate": "lg validate"
  },
  "devDependencies": {
    "@babel/core": "7.21.8",
    "@changesets/cli": "^2.26.0",
    "@lg-tools/cli": "*",
    "@lg-tools/meta": "*",
    "@lg-tools/slackbot": "*",
    "@lg-tools/storybook": "*",
    "@lg-tools/storybook-decorators": "*",
    "@types/lodash": "^4.14.170",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@types/react-transition-group": "^4.4.6",
    "chromatic": "^6.17.2",
    "dotenv": "^10.0.0",
    "gh-pages": "^3.1.0",
    "npm-run-all": "^4.1.5",
    "prop-types": "^15.7.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "5.2.0",
    "storybook": "7.1.1",
    "turbo": "^1.10.7",
    "typescript": "~4.7.0"
  },
  "resolutions": {
    "caniuse-lite": "^1.0.30001135",
    "@types/react-transition-group/@types/react": "^17.0.2"
  },
  "workspaces": [
    "packages/*",
    "tools/*"
  ],
  "keywords": [
    "mongodb",
    "ui",
    "kit",
    "components",
    "react",
    "uikit",
    "leafygreen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/leafygreen-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mongodb/leafygreen-ui/issues"
  }
}
