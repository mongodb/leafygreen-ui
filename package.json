{
  "description": "leafyGreen UI Kit",
  "license": "Apache-2.0",
  "private": true,
  "engines": {
    "node": ">= 14.0.0"
  },
  "keywords": [
    "mongodb",
    "ui",
    "kit",
    "components",
    "react",
    "uikit",
    "leafygreen"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/leafygreen-ui.git"
  },
  "bugs": {
    "url": "https://github.com/mongodb/leafygreen-ui/issues"
  },
  "scripts": {
    "start": "start-storybook -p 9001 -c storybook -s ./storybook/static --no-version-updates",
    "pre-build": "lerna run --parallel pre-build -- --env production",
    "build": "yarn pre-build && lerna run --parallel build",
    "build:storybook": "build-storybook -c storybook -o ./storybook/public -s ./storybook/static",
    "clean": "rimraf packages/*/{dist,node_modules} node_modules **/*.tsbuildinfo",
    "create-package": "node ./scripts/package-builder.js",
    "fix": "yarn prettier:fix && yarn eslint:fix",
    "init": "yarn && yarn build && yarn ts:build --verbose",
    "link-all-packages": "lerna exec --ignore @leafygreen-ui/lib --ignore @leafygreen-ui/theme --ignore @leafygreen-ui/mongo-nav -- yarn link",
    "link": "./scripts/link.sh",
    "eslint:check": "eslint \"**/*.{js,ts,tsx}\"",
    "eslint:fix": "eslint \"**/*.{js,ts,tsx}\" --fix",
    "prettier:fix": "prettier --write \"**/*.{js,ts,tsx,json,md}\"",
    "prettier:check": "prettier --check \"**/*.{js,ts,tsx,json,md}\"",
    "test": "jest --env jsdom --config jest.config.js",
    "test:ssr": "jest --env jsdom --config ssr.jest.config.js",
    "ts": "tsc --project tsconfig.json",
    "ts:build": "tsc --build build.tsconfig.json",
    "ts:downlevel": "rimraf packages/*/dist/ts3.4 && echo packages/*/dist | NODE_NO_WARNINGS=1 xargs -I{} -n1 -P8 downlevel-dts {} {}/ts3.4",
    "prepublishOnly": "yarn lint && yarn test && yarn build && yarn ts:downlevel",
    "release": "yarn changeset publish --public",
    "release:site": "gh-pages --dist storybook/public",
    "demo:site": "./scripts/publish-storybook.sh"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-proposal-export-default-from": "7.8.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.8.3",
    "@babel/plugin-proposal-logical-assignment-operators": "7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "7.9.5",
    "@babel/plugin-proposal-optional-chaining": "7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "7.9.5",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "^7.9.0",
    "@babel/register": "7.9.0",
    "@babel/runtime": "7.9.2",
    "@changesets/cli": "^2.6.2",
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "@leafygreen-ui/lib": "^6.1.0",
    "@leafygreen-ui/testing-lib": "*",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-url": "^5.0.1",
    "@storybook/addon-a11y": "^5.3.18",
    "@storybook/addon-knobs": "^5.3.18",
    "@storybook/react": "^5.3.18",
    "@storybook/theming": "^5.3.18",
    "@svgr/babel-plugin-replace-jsx-attribute-value": "^5.0.1",
    "@svgr/cli": "^5.3.1",
    "@svgr/core": "^5.3.1",
    "@svgr/rollup": "^5.4.0",
    "@svgr/webpack": "^5.3.1",
    "@testing-library/dom": "^7.2.1",
    "@testing-library/jest-dom": "^5.4.0",
    "@testing-library/react": "^10.0.3",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^12.1.8",
    "@types/facepaint": "^1.2.1",
    "@types/highlight.js": "^9.12.3",
    "@types/jest": "^26.0.0",
    "@types/lodash": "^4.14.149",
    "@types/meow": "^5.0.0",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-transition-group": "^4.2.4",
    "@types/storybook__react": "^5.2.1",
    "@types/xml2json": "^0.11.0",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "26.0.0",
    "babel-loader": "8.1.0",
