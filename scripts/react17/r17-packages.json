{
  "$schema": "https://json.schemastore.org/package",
  "description": "Dependencies needed to build & test in a React 17 environment",
  "dependencies": {
    "@storybook/react": "7.6.20",
    "@testing-library/react": "11.2.7",
    "@testing-library/react-hooks": "8.0.1",
    "@testing-library/user-event": "13.5.0",
    "@types/react": "16.9.56",
    "@types/react-dom": "16.9.18",
    "@types/react-is": "17.0.7",
    "chromatic": "11.27.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-test-renderer": "17.0.2",
    "storybook": "7.6.20",
    "typescript": "5.0.4",
    "@lg-tools/storybook-addon": "0.2.8",
    "@lg-tools/storybook-utils": "0.1.3"
  },
  "pnpm": {
    "overrides": {
      "@emotion/react": "11.11.1",
      "@storybook/react": "7.6.20",
      "@storybook/test": "7.6.20",
      "@storybook/types": "7.6.20",
      "@storybook/csf": "0.1.13",
      "@types/react": "16.9.56",
      "@types/react-dom": "16.9.18",
      "@types/react-is": "17.0.7",
      "@testing-library/jest-dom": "5.17.0",
      "@testing-library/react": "11.2.7",
      "@testing-library/react-hooks": "8.0.1",
      "@testing-library/user-event": "13.5.0",
      "jackspeak": "2.1.1",
      "react-test-renderer": "17.0.2",
      "react": "$react",
      "react-dom": "$react-dom",
      "react-is": "17.0.2",
      "typescript": "5.0.4"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "@types/react": "16",
        "@types/react-dom": "16",
        "react": "17",
        "react-dom": "17"
      }
    }
  }
}
