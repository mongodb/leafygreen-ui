{
  "name": "@lg-tools/storybook-addon",
  "version": "0.6.5",
  "description": "Storybook Addon for LeafyGreen UI Kit",
  "license": "Apache-2.0",
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "types@<=5.0": "./dist/types/ts4.9/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.js"
    },
    "./main": {
      "types": "./dist/types/main.d.ts",
      "import": "./dist/esm/main.js",
      "require": "./dist/umd/main.js"
    },
    "./manager": {
      "types": "./dist/types/manager.d.ts",
      "import": "./dist/esm/manager.js",
      "require": "./dist/umd/manager.js"
    },
    "./preview": {
      "types": "./dist/types/preview.d.ts",
      "import": "./dist/esm/preview.js",
      "require": "./dist/umd/preview.js"
    },
    "./preset": "./preset.js",
    "./package.json": "./package.json",
    "./static": "./static"
  },
  "files": [
    "dist/**/*",
    "static/*",
    "*.js",
    "*.d.ts",
    "README.md",
    "package.json",
    "preset.js"
  ],
  "scripts": {
    "build": "lg-build bundle",
    "tsc": "lg-build tsc",
    "upgrade": "npx storybook@latest upgrade --config-dir ."
  },
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/mongodb/leafygreen-ui/tree/main/packages/storybook-addon",
  "repository": {
    "type": "git",
    "url": "https://github.com/mongodb/leafygreen-ui"
  },
  "bugs": {
    "url": "https://jira.mongodb.org/projects/PD/summary"
  },
  "dependencies": {
    "@leafygreen-ui/lib": "workspace:^",
    "@leafygreen-ui/palette": "workspace:^",
    "@leafygreen-ui/tokens": "workspace:^",
    "@leafygreen-ui/typography": "workspace:^",
    "@lg-tools/build": "workspace:^",
    "@lg-tools/meta": "workspace:^",
    "@lg-tools/storybook-decorators": "workspace:^",
    "@lg-tools/storybook-utils": "workspace:^",
    "@mdx-js/react": "2.3.0",
    "@storybook/addon-a11y": "9.1.16",
    "@storybook/addon-links": "9.1.16",
    "@storybook/react": "9.1.16",
    "@storybook/react-webpack5": "9.1.16",
    "@svgr/webpack": "8.0.1",
    "assert": "^2.1.0",
    "babel-loader": "9.2.1",
    "buffer": "6.0.3",
    "crypto-browserify": "^3.12.1",
    "events": "^3.3.0",
    "file-loader": "6.2.0",
    "glob": "10.3.12",
    "lodash": "4.17.21",
    "node-polyfill-webpack-plugin": "^4.1.0",
    "os-browserify": "0.3.0",
    "path-browserify": "1.0.1",
    "process": "^0.11.10",
    "react-docgen-typescript": "2.2.2",
    "storybook-dark-mode": "^4.0.2",
    "stream-browserify": "3.0.0",
    "string-replace-loader": "^3.1.0",
    "string_decoder": "^1.3.0",
    "tty-browserify": "0.0.1",
    "url": "^0.11.4",
    "url-loader": "4.1.1",
    "util": "^0.12.5",
    "webpack": "5.88.0"
  },
  "peerDependencies": {
    "@storybook/react": "9.1.16",
    "react": "^17.0.0 || ^18.0.0",
    "react-dom": "^17.0.0 || ^18.0.0",
    "storybook": "9.1.16"
  },
  "typesVersions": {}
}
