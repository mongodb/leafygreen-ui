{
  "name": "@lg-tools/storybook-decorators",
  "version": "1.0.5",
  "description": "Storybook decorators used in LeafyGreen repos",
  "main": "./dist/umd/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/types/index.d.ts",
  "license": "Apache-2.0",
  "scripts": {
    "build": "lg-build bundle",
    "tsc": "lg-build tsc",
    "upgrade": "npx storybook@latest upgrade --config-dir ../storybook"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@leafygreen-ui/banner": "workspace:^",
    "@leafygreen-ui/emotion": "workspace:^",
    "@leafygreen-ui/palette": "workspace:^",
    "@leafygreen-ui/tokens": "workspace:^",
    "@lg-tools/build": "workspace:^",
    "@lg-tools/storybook-utils": "workspace:^",
    "@storybook/react": "8.6.14",
    "lodash": "4.17.21"
  },
  "peerDependencies": {
    "@leafygreen-ui/leafygreen-provider": "workspace:^5.0.0 || ^4.0.0 || ^3.2.0"
  },
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "types@<=5.0": "./dist/types/ts4.9/index.d.ts",
      "import": "./dist/esm/index.js",
      "require": "./dist/umd/index.js"
    }
  },
  "typesVersions": {}
}
